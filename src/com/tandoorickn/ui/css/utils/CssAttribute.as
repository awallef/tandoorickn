package  com.tandoorickn.ui.css.utils{	import com.tandoorickn.ui.css.core.CoreCssSprite;		public class CssAttribute {		public static function setAttribute(cssSprite:CoreCssSprite):void		{			cssSprite.alpha = cssSprite.style.alpha;			cssSprite.visible = cssSprite.style.visible;			cssSprite.mouseEnabled = cssSprite.style.mouseEnable;			cssSprite.buttonMode = cssSprite.style.buttonMode;			if(cssSprite.buttonMode) cssSprite.mouseEnabled = cssSprite.style.mouseEnable = true;			cssSprite.filters = cssSprite.style.filters;		}	}	}