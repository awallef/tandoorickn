package  com.tandoorickn.patterns.workflow{	import com.tandoorickn.interfaces.patterns.IWorkflow;	import com.tandoorickn.patterns.command.AbstractCommand;		import org.puremvc.as3.interfaces.*;
		public class AbstractWorkflow extends AbstractCommand implements IWorkflow	{		public static const	NAME			:String			= "AbstractWorkflow";				public function AbstractWorkflow()		{		}				public function configure(notification:INotification):void		{					}				public function initWorkflow(notification:INotification):void		{			initCommands();			initProcess();			initMacros();		}				public function removeWorkflow():void		{			removeCommands();			removeProcess();			removeMacros();		}				override public function execute(notification:INotification):void		{					}				public function initCommands():void			{}				public function initProcess():void			{}				public function initMacros():void			{}				override public function revision(notification:INotification):void		{					}				public function removeCommands():void		{}				public function removeProcess():void		{}				public function removeMacros():void			{}	}	}