package com.tandoorickn.patterns.observer{	import org.puremvc.as3.interfaces.*;	import com.tandoorickn.patterns.facade.AbstractFacade;		public class AbstractNotifier implements INotifier	{		public var facade  :AbstractFacade;				public function sendNotification( notificationName:String, body:Object=null, type:String=null ):void 		{			facade.sendNotification( notificationName, body, type );		}			}}