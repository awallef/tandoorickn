package com.tandoorickn.utils.random{	public class RandomChar	{		// 48 -> 57 numbers		// 65 -> 90 abc		// 97 -> 122 ABC				public static function randomiz(length:uint):String		{			var str:String="";			for(var i:uint = 0; i < length  ; i++)			{				var j:uint = Math.random() * 3;				switch(j)				{					case 0:						str += String.fromCharCode(RandomChar.randi(48,57));						break;											case 1:						str += String.fromCharCode(RandomChar.randi(65,90));						break;											case 2:						str += String.fromCharCode(RandomChar.randi(97,122));						break;				}							}			return str;		}				public static function rand(min:Number, max:Number):Number { return (Math.random() * (max-min)) + min; }		public static function randi(min:int, max:int):int { return (Math.random() * (max-min)) + min; }		public static function randa(a:Array):Object { return a.splice(randi(0, a.length), 1)[0]; }	}}