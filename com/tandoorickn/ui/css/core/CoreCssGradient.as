package  com.tandoorickn.ui.css.core{	import flash.geom.Matrix;		public dynamic class CoreCssGradient {		public  var	fillType			:String,			 		colors				:Array,			 		alphas				:Array,			 		ratios				:Array,			 		matr				:Matrix,			 		spreadMethod		:String,					rotation			:Number		= 0,					tx					:Number		= 0,					ty					:Number		= 0;							public function merge(obj:Object):void		{			for( var i:String in obj) this[i] = obj[i]; 		}				public function CoreCssGradient(config:Object = null)		{			this.merge(config);		}	}	}