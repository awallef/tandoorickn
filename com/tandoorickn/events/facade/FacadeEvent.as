package  com.tandoorickn.events.facade{	import flash.events.Event;		public class FacadeEvent extends Event	{		public static const				ERROR:String				= "FacadeEvent.ERROR";				public static const				INITIALIZED:String			= "FacadeEvent.INITIALIZED";						public var 	noteName			:String,					noteBody			:Object,					noteType			:String,					code				:String,					message				:String;									public function FacadeEvent(type:String,code:String,message:String,noteName:String,noteBody:Object,noteType:String,bubbles:Boolean = false, cancelable:Boolean = true)		{			super(type,bubbles,cancelable);						this.noteName = noteName;			this.noteBody = noteBody;			this.noteType = noteType;			this.code = code;			this.message = message;		}	}	}